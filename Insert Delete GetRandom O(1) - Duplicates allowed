class RandomizedCollection {
    Random rand;
    List<Integer> list;
    Map<Integer, Set<Integer>> map;

    public RandomizedCollection() {
        rand = new Random();
        list = new ArrayList<>();
        map = new HashMap<>();
    }
    
    public boolean insert(int val) {
        if(!map.containsKey(val)){
            map.put(val, new HashSet<>());
        }
        map.get(val).add(list.size());
        list.add(val);
        return map.get(val).size() == 1;
    }
    
    public boolean remove(int val) {
        if(!map.containsKey(val)){
            return false;
        }

        // Get the last element in the list
        int valueAtLastIndex = list.get(list.size()-1);
         // Get one index where val exists
        int idxToRemove = map.get(val).iterator().next();

        map.get(val).remove(idxToRemove);

        // Replace val with lastVal in the list
        list.set(idxToRemove, valueAtLastIndex);

        // Update the map for lastVal
        map.get(valueAtLastIndex).remove(list.size() - 1);
        map.get(valueAtLastIndex).add(idxToRemove);

        // clean val
        list.remove(list.size()-1);
        
        return true;
    }
    
    public int getRandom() {
        return list.get(rand.nextInt(list.size()));
    }
}

/**
 * Your RandomizedCollection object will be instantiated and called as such:
 * RandomizedCollection obj = new RandomizedCollection();
 * boolean param_1 = obj.insert(val);
 * boolean param_2 = obj.remove(val);
 * int param_3 = obj.getRandom();
 */
